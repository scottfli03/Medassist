                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Visits.VisitID, Visits.VisitDate, Visits.NurseID, Visits.PatientID, Visits.DoctorID, Visits.Diagnosis, Visits.Systolic, Visits.Diastolic, Visits.Temperature, Visits.RespirationRate, Visits.HeartRate, Visits.Symptoms, 
                         Orders.OrderID, Tests.TestName, Doctors.FirstName AS DoctorFirst, Doctors.LastName AS DoctorLast, Nurses.FirstName AS NursesFirst, Nurses.LastName AS NursesLast, Patients.FirstName, 
                         Patients.LastName, Patients.MInit, Patients.DOB, Patients.Gender
FROM            Visits INNER JOIN
                         Orders ON Visits.VisitID = Orders.VisitID INNER JOIN
                         Tests ON Orders.TestID = Tests.TestID INNER JOIN
                         Patients ON Visits.PatientID = Patients.PatientID INNER JOIN
                         Employees AS Nurses ON Visits.NurseID = Nurses.EmployeeID LEFT OUTER JOIN
                         Employees AS Doctors ON Visits.DoctorID = Doctors.EmployeeID
WHERE        (Visits.VisitDate BETWEEN @visitStart AND @visitEnd)
ORDER BY Visits.VisitDate</CommandText>
